{
  "name": "starter-webpack",
  "version": "1.0.0",
  "description": "My Enonic XP project",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build:client": "webpack --config webpack.client.config.js --color",
    "build:server": "webpack --config webpack.server.config.js --color",
    "build": "run-p -c build:*",
    "dist:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --color",
    "dist:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --color --hide-modules",
    "dist": "run-p -c dist:*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/preset-env": "^7.18.0",
    "@babel/register": "^7.17.7",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "browserslist-config-enonic": "^1.0.7",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.9",
    "file-loader": "^6.2.0",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.2",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-normalize": "^10.0.1",
    "ramda": "^0.28.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.10.0"
  }
}
